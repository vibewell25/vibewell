{
  "name": "vibewell",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "mobile"
  ],
  "description": "Beauty and wellness booking platform with advanced AI features",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "dev:server": "node server/index.js",
    "dev:mobile": "cd mobile && npm install --legacy-peer-deps && npx expo start --tunnel --web",
    "dev:full": "npx concurrently \"npm run dev\" \"PORT=4001 npm run dev:server\" \"npm run dev:mobile\"",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --config jest.config.js --ci --coverage",
    "test:update": "jest --config jest.config.js -u",
    "test:smoke": "jest -c jest-config/smoke.config.js",
    "test:smoke:watch": "jest -c jest-config/smoke.config.js --watch",
    "test:smoke:coverage": "jest -c jest-config/smoke.config.js --coverage",
    "test:smoke:ci": "jest -c jest-config/smoke.config.js --ci --coverage",
    "test:smoke:update": "jest -c jest-config/smoke.config.js -u",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "jest --testMatch='**/*.a11y.test.{ts,tsx}'",
    "test:performance": "jest --testMatch='**/*.perf.test.{ts,tsx}'",
    "test:visual": "jest --testMatch='**/*.visual.test.{ts,tsx}'",
    "test:integration": "jest --testMatch='**/*.integration.test.{ts,tsx}'",
    "test:enhanced": "jest --config jest.config.enhanced.ts",
    "test:a11y": "jest --testMatch='**/*.a11y.test.{ts,tsx}'",
    "test:pa11y": "pa11y-ci",
    "lint": "next lint",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --ignore-pattern \"**/__tests__/**\" --ignore-pattern \"**/*.test.ts\" --ignore-pattern \"**/*.test.tsx\" --ignore-pattern \"**/*.spec.ts\" --ignore-pattern \"**/*.spec.tsx\" --fix",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "setup:auth0": "node scripts/setup-auth0.js",
    "setup:s3": "node scripts/setup-s3.js",
    "setup:stripe": "node scripts/setup-stripe.js",
    "analyze": "cross-env ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze:components": "node scripts/analyze-components.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "build:optimize": "cross-env NODE_ENV=production next build"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.5.1",
    "@chakra-ui/react": "3.16.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "2.2.0",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@mui/material": "^5.15.10",
    "@prisma/client": "6.7.0",
    "@react-three/drei": "9.96.3",
    "@react-three/fiber": "8.15.19",
    "@react-spring/three": "9.7.3",
    "@tensorflow/tfjs": "^4.17.0",
    "@tensorflow/tfjs-backend-webgl": "^4.17.0",
    "@tensorflow/tfjs-converter": "^4.17.0",
    "@tensorflow/tfjs-core": "^4.17.0",
    "@types/express": "5.0.1",
    "@types/ioredis": "4.28.10",
    "@types/request-ip": "0.0.41",
    "@types/three": "^0.161.2",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "csrf-csrf": "4.0.0",
    "express": "5.1.0",
    "face-api.js": "^0.22.2",
    "framer-motion": "10.16.4",
    "ioredis": "5.6.1",
    "nanoid": "5.1.5",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "openai": "4.96.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.45.1",
    "react-hot-toast": "2.5.2",
    "request-ip": "3.3.0",
    "three": "^0.161.0",
    "winston": "3.17.0",
    "zod": "3.22.4",
    "zustand": "4.4.1",
    "webxr-polyfill": "^2.0.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.3.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.2.5",
    "prisma": "6.7.0",
    "react-test-renderer": "18.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "next": "13.5.11",
    "next-auth": "4.24.11",
    "@react-spring/three": "9.7.3",
    "@react-three/fiber": "8.15.19",
    "zod": "3.22.4",
    "three-mesh-bvh": "0.8.0"
  },
  "overrides": {
    "micromatch": "^4.0.8",
    "node-fetch": "^2.6.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vibewell.git"
  },
  "keywords": [
    "vibewell",
    "typescript",
    "third-party-integrations",
    "web-app"
  ],
  "author": "Your Name",
  "license": "MIT"
}
