    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/validator": "^13.15.0",
    "@types/winston": "^2.4.4",
    "@vitejs/plugin-react": "^4.4.1",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.57.0",
