{
  "name": "vibewell-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:tests": "eslint \"src/**/*.test.{ts,tsx}\" --fix",
    "test": "jest --config jest.config.js || true",
    "test:ci": "jest --config jest.config.js --ci --coverage",
    "cleanup": "node scripts/cleanupInstrumentation.js",
    "fix:tests": "npx jscodeshift -t scripts/fixTestsTransform.js --extensions ts,tsx --parser tsx src/**/*.{test,spec}.{ts,tsx}",
    "fix:tests:ast": "node scripts/fixTestsWithTSMorph.js",
    "clean:tests": "npm run cleanup && bash scripts/clean-tests.sh",
    "generate:tests": "node scripts/generateTestStubs.js"
  },
  "dependencies": {
    "@radix-ui/react-toast": "^1.2.11",
    "@simplewebauthn/browser": "^13.1.0",
    "@types/howler": "^2.2.12",
    "date-fns": "^4.1.0",
    "howler": "^2.2.4",
    "i18next": "^25.0.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "jest-axe": "^10.0.0",
    "next": "^15.3.1",
    "next-auth": "^4.24.11",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "^15.5.1",
    "recoil": "^0.7.7",
    "redoc": "^2.5.0"
  },
  "devDependencies": {
    "@radix-ui/react-avatar": "^1.1.7",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-jest": "^27.0.0",
    "glob": "^8.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jscodeshift": "^17.3.0",
    "msw": "^2.7.6",
    "prettier": "^2.0.0",
    "ts-morph": "^24.0.0",
    "ts-node": "^10.9.1"
  }
}
