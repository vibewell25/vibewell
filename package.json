{
  "name": "vibewell",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "description": "Beauty and wellness booking platform with advanced AI features",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev --workspace vibewell-web",
    "dev:server": "node apps/server/index.js",
    "dev:mobile": "cd apps/mobile && npm install --legacy-peer-deps && npx expo start --tunnel --web",
    "dev:full": "npx concurrently \"npm run dev\" \"PORT=4001 npm run dev:server\" \"npm run dev:mobile\"",
    "dev:monitoring": "docker-compose -f docker-compose.dev.yml up -d",
    "build": "npm run build --workspace vibewell-web",
    "start": "npm run start --workspace vibewell-web",
    "format": "prettier --write \"apps/web/src/**/*.{js,jsx,ts,tsx,css,md,json}\" \"packages/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"apps/web/src/**/*.{js,jsx,ts,tsx,css,md,json}\" \"packages/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --config jest.config.js --ci --coverage",
    "test:update": "jest --config jest.config.js -u",
    "test:smoke": "jest -c jest-config/smoke.config.js",
    "test:smoke:watch": "jest -c jest-config/smoke.config.js --watch",
    "test:smoke:coverage": "jest -c jest-config/smoke.config.js --coverage",
    "test:smoke:ci": "jest -c jest-config/smoke.config.js --ci --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "jest --testMatch='**/*.a11y.test.{ts,tsx}'",
    "test:performance": "jest --testMatch='**/*.perf.test.{ts,tsx}'",
    "test:visual": "jest --testMatch='**/*.visual.test.{ts,tsx}'",
    "test:integration": "jest --testMatch='**/*.integration.test.{ts,tsx}'",
    "test:enhanced": "jest --config jest.config.enhanced.ts",
    "test:a11y": "jest --testMatch='**/*.a11y.test.{ts,tsx}'",
    "test:pa11y": "pa11y-ci",
    "lint": "eslint \"apps/web/src/**/*.{js,jsx,ts,tsx}\" \"packages/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"apps/web/src/**/*.{js,jsx,ts,tsx}\" \"packages/**/*.{js,jsx,ts,tsx}\" --fix",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "setup:s3": "node scripts/setup-s3.js",
    "setup:stripe": "node scripts/setup-stripe.js",
    "analyze": "cross-env ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze:components": "node scripts/analyze-components.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "build:optimize": "cross-env NODE_ENV=production next build",
    "monitor:resources": "node scripts/monitor-resources.js",
    "monitor:api": "node scripts/monitor-api.js",
    "monitor:performance": "node scripts/monitor-performance.js",
    "analyze:bundle": "ANALYZE=true next build",
    "test:load": "k6 run tests/load-tests/main.js",
    "test:security": "snyk test",
    "validate:env": "node scripts/validate-env.js",
    "health:check": "node scripts/health-check.js",
    "security:monitor": "node scripts/security-monitor.js",
    "security:audit": "npm audit && npm outdated",
    "security:fix": "node scripts/security-fix.js && npm audit fix",
    "security:scan": "node scripts/fix-integer-overflow.js && gitleaks detect --config .gitleaks.toml",
    "security:hardcoded": "gitleaks detect --config .gitleaks.toml",
    "security:memory": "node --max-old-space-size=512 --expose-gc scripts/memory-check.js",
    "security:full": "npm run security:fix && npm run security:scan && npm run security:audit && npm run security:monitor",
    "deploy:validate": "node scripts/validate-deployment.js",
    "deploy:canary": "vercel --scope=team_name",
    "deploy:prod": "vercel --prod --scope=team_name",
    "metrics:dashboard": "open http://localhost:3001/dashboards",
    "prometheus": "open http://localhost:9090",
    "jaeger": "open http://localhost:16686",
    "mobile:build": "cd mobile && eas build --platform all --profile preview",
    "mobile:update": "cd mobile && eas update --branch preview",
    "mobile:publish": "cd mobile && eas update --branch production",
    "mobile:submit": "cd mobile && eas submit --platform all",
    "mobile:build:android": "cd mobile && eas build --platform android --profile preview",
    "mobile:build:ios": "cd mobile && eas build --platform ios --profile preview",
    "generate:services": "node scripts/generate-services.js",
    "prebuild": "npm run generate:services && npm run prebuild",
    "clean:tests:web": "npm run clean:tests --workspace vibewell-web",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.7.0",
    "@chakra-ui/react": "3.16.1",
    "@emotion/react": "^11.11.4",
