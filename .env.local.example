# Authentication
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_URL=http://localhost:3000

# OAuth Providers
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GITHUB_ID=your-github-id
GITHUB_SECRET=your-github-secret

# Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your-ga-id

# Push Notifications
NEXT_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-firebase-auth-domain
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-firebase-project-id
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-firebase-messaging-sender-id
NEXT_PUBLIC_FIREBASE_APP_ID=your-firebase-app-id

# Storage
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=your-cloudinary-upload-preset

# API Keys
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# Rate Limiting
RATE_LIMIT_MAX=100
GRAPHQL_COMPLEXITY_LIMIT=1000

# Website URL 
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Sample .env.local file for load testing with Redis
NODE_ENV=production

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_TLS=false
REDIS_TIMEOUT=5000

# Rate Limiting Configuration
RATE_LIMIT_MODE=redis
REDIS_ENABLED=true

# Rate Limits
DEFAULT_RATE_LIMIT_MAX=100
DEFAULT_RATE_LIMIT_WINDOW=60
AUTH_RATE_LIMIT_MAX=20
AUTH_RATE_LIMIT_WINDOW=60
SENSITIVE_RATE_LIMIT_MAX=10
SENSITIVE_RATE_LIMIT_WINDOW=60
ADMIN_RATE_LIMIT_MAX=50
ADMIN_RATE_LIMIT_WINDOW=60

# Logging
LOG_LEVEL=info
ENABLE_RATE_LIMIT_LOGGING=true
LOG_SUSPICIOUS_ACTIVITY=true

# Copy this file to .env.local for load testing
# Make sure Redis is running on localhost:6379 