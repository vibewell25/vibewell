    "eslint-plugin-sonarjs": "^3.0.2",
    "express": "5.1.0",
    "face-api.js": "^0.20.1",
    "framer-motion": "^10.16.4",
    "graphql": "^16.11.0",
    "ioredis": "5.6.1",
    "isomorphic-dompurify": "^2.24.0",
    "lucide-react": "^0.503.0",
    "nanoid": "5.1.5",
    "next": "^15.3.1",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "openai": "^4.28.4",
    "perf_hooks": "^0.0.1",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.45.1",
    "react-hot-toast": "2.5.2",
    "react-icons": "^5.0.1",
    "react-redux": "^9.2.0",
    "request-ip": "3.3.0",
