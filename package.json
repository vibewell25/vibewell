{
  "name": "vibewell",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:scripts": "tsc --project scripts/tsconfig.json",
    "build:docs-generator": "tsc --project scripts/tsconfig.document-gen.json",
    "build:all": "npm run build && npm run build:scripts && npm run build:docs-generator",
    "start": "next start",
    "lint": "next lint",
    "lint:style": "node scripts/lint-codebase.js",
    "lint:style:fix": "node scripts/lint-codebase.js --fix",
    "lint:style:report": "node scripts/lint-codebase.js --report",
    "docs:generate": "node scripts/generate-component-docs.js",
    "docs:validate": "node scripts/generate-component-docs.js --validate src/components",
    "docs:fix": "node scripts/generate-component-docs.js --validate src/components --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "prepare": "husky install || echo 'Husky not installed, skipping hooks'",
    "test": "jest",
    "test:all": "./scripts/run-all-tests.sh",
    "test:unit": "jest 'src/**/*.test.(ts|tsx)'",
    "test:node": "jest --config=jest.smoke.config.js",
    "test:smoke": "jest --config=jest.smoke.config.js tests/smoke",
    "test:rate-limiting": "jest --config=jest.smoke.config.js tests/rate-limiting",
    "test:ar": "node scripts/test-ar-optimizations.js",
    "test:wizard": "npx playwright test src/scripts/test-business-profile-wizard.ts",
    "test:performance": "node scripts/performance-testing.js",
    "test:performance:api": "node scripts/performance-testing.js --endpoint=/api/services",
    "test:load": "node scripts/performance-testing.js --concurrent=50 --duration=30",
    "security-audit": "node scripts/security-audit.js",
    "analyze:frontend": "node scripts/frontend-performance.js",
    "analyze:frontend:mobile": "node scripts/frontend-performance.js --mobile",
    "analyze:frontend:desktop": "node scripts/frontend-performance.js --desktop",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:performance": "cypress run --spec 'cypress/e2e/performance-testing.cy.ts'",
    "test:e2e": "playwright test",
    "test:post-deploy": "jest --config jest.e2e.config.js tests/post-deploy",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:enhanced": "jest --config jest.enhanced.config.js",
    "test:enhanced:watch": "jest --config jest.enhanced.config.js --watch",
    "test:enhanced:coverage": "jest --config jest.enhanced.config.js --coverage",
    "test:setup": "./scripts/install-test-dependencies.sh",
    "migrate:rate-limiter": "node scripts/migrate-rate-limiter-imports.js",
    "migrate:rate-limiter:guided": "./scripts/run-rate-limiter-migration.sh",
    "type-check": "tsc --noEmit",
    "type-check:src": "tsc --noEmit",
    "type-check:app": "tsc --project src/app/tsconfig.json --noEmit",
    "type-check:components": "tsc --project src/components/tsconfig.json --noEmit",
    "type-check:scripts": "tsc --project scripts/tsconfig.json --noEmit",
    "type-check:docs": "tsc --project scripts/tsconfig.document-gen.json --noEmit",
    "type-check:tests": "tsc --project src/test-utils/tsconfig.json --noEmit",
    "type-check:utils": "tsc --project src/utils/tsconfig.json --noEmit",
    "type-check:all": "npm run type-check:scripts && npm run type-check:docs && npm run type-check:tests && npm run type-check:utils && npm run type-check:app && npm run type-check:components",
    "backup:monitor": "ts-node src/scripts/monitor-backups.ts",
    "backup:init": "curl http://localhost:3000/api/backup/init",
    "security:scan": "snyk test",
    "security:monitor": "ts-node scripts/security-monitor.ts",
    "security:daily": "node -e \"require('node-cron').schedule('0 0 * * *', () => require('./scripts/security-monitor')())\"",
    "security:eslint": "eslint --ext .js,.jsx,.ts,.tsx --config .eslintrc.js src/ --rule \"security/detect-object-injection: 2\"",
    "security:audit": "npm audit --audit-level=moderate",
    "security:all": "npm run security:audit && npm run security:eslint && npm run security:scan && npm run security:monitor",
    "audit:generate-report": "ts-node scripts/generate-audit-report.ts",
    "audit:security": "curl -X POST http://localhost:3000/api/audit -H \"Content-Type: application/json\" -d '{\"action\":\"run_category_audit\",\"category\":\"security\"}'",
    "audit:performance": "curl -X POST http://localhost:3000/api/audit -H \"Content-Type: application/json\" -d '{\"action\":\"run_category_audit\",\"category\":\"performance\"}'",
    "audit:ux": "curl -X POST http://localhost:3000/api/audit -H \"Content-Type: application/json\" -d '{\"action\":\"run_category_audit\",\"category\":\"ux\"}'",
    "audit:compliance": "curl -X POST http://localhost:3000/api/audit -H \"Content-Type: application/json\" -d '{\"action\":\"run_category_audit\",\"category\":\"compliance\"}'",
    "audit:booking": "curl -X POST http://localhost:3000/api/audit -H \"Content-Type: application/json\" -d '{\"action\":\"run_category_audit\",\"category\":\"booking\"}'",
    "audit:comprehensive": "curl -X POST http://localhost:3000/api/audit -H \"Content-Type: application/json\" -d '{\"action\":\"run_comprehensive_audit\"}'",
    "audit:test": "jest src/__tests__/audit/audit-service.test.ts",
    "test:integration": "jest 'tests/integration/**/*.test.(ts|tsx)'",
    "perf:monitor": "ts-node scripts/performance-monitor.ts",
    "perf:analyze": "ANALYZE=true next build",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output-path=./reports/lighthouse.html --view",
    "perf:bundle": "node scripts/analyze-bundle.js",
    "perf:bundle:watch": "nodemon scripts/analyze-bundle.js",
    "perf:all": "npm run build && npm run perf:monitor && npm run perf:analyze && npm run perf:bundle && npm run perf:lighthouse"
  },
